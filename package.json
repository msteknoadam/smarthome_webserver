{
	"name": "smarthome_webserver",
	"version": "1.0.0",
	"description": "This is the repo for Smart Home Project's webserver files.",
	"main": "server.ts",
	"scripts": {
		"install:server": "npm i",
		"install:raspclient": "python3 -m pip install --upgrade -r ./raspberryclient/requirements.txt",
		"start:server": "nodemon --exec node_modules/.bin/ts-node -- ./server/server.ts",
		"start:raspiclient": "python3 ./raspberryclient/client.py"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/msteknoadam/smarthome_webserver.git"
	},
	"keywords": [
		"socketio"
	],
	"author": "TEKNO",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/msteknoadam/smarthome_webserver/issues"
	},
	"homepage": "https://github.com/msteknoadam/smarthome_webserver#readme",
	"devDependencies": {
		"typescript": "^3.5.3"
	},
	"dependencies": {
		"@types/express": "^4.17.0",
		"@types/socket.io": "^2.1.2",
		"express": "^4.17.1",
		"nodemon": "^1.19.1",
		"socket.io": "^2.2.0",
		"ts-node": "^8.3.0"
	}
}
